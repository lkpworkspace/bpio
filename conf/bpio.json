{
    "_lib":"libbpio.so",
    "_func":{
        "open_file":{
            "_input":[
                {
                    "type":"bpbase.String",
                    "name":"file path"
                },
                {
                    "type":"bpbase.String",
                    "name":"mode"
                }
            ],
            "_output":[
                {
                    "type":"bpbase.Int",
                    "name":"handle"
                }
            ]
        },
        "read_file":{
            "_input":[
                {
                    "type":"bpbase.Int",
                    "name":"handle"
                },
                {
                    "type":"bpbase.Int",
                    "name":"size"
                }
            ],
            "_output":[
                {
                    "type":"bpbase.Int",
                    "name":"read size"
                },
                {
                    "type":"bpbase.String",
                    "name":"data"
                }
            ]
        },
        "write_file":{
            "_input":[
                {
                    "type":"bpbase.Int",
                    "name":"handle"
                },
                {
                    "type":"bpbase.String",
                    "name":"data"
                }
            ],
            "_output":[
                {
                    "type":"bpbase.Int",
                    "name":"write size"
                }
            ]
        },
        "close_file":{
            "_input":[
                {
                    "type":"bpbase.Int",
                    "name":"handle"
                }
            ],
            "_output":[]
        }
    }
}